{
  "number": 109,
  "name": "Al-Kafirun",
  "totalVerses": 6,
  "verses": [
    {
      "number": 1,
      "arabic": "  قُلۡ یَـٰۤأَیُّهَا ٱلۡكَـٰفِرُونَ\n",
      "translation": "Say, \"O disbelievers,",
      "audioFile": "audio/109/109001.mp3"
    },
    {
      "number": 2,
      "arabic": "لَاۤ أَعۡبُدُ مَا تَعۡبُدُونَ\n",
      "translation": "I do not worship what you worship.",
      "audioFile": "audio/109/109002.mp3"
    },
    {
      "number": 3,
      "arabic": "وَلَاۤ أَنتُمۡ عَـٰبِدُونَ مَاۤ أَعۡبُدُ\n",
      "translation": "Nor are you worshippers of what I worship.",
      "audioFile": "audio/109/109003.mp3"
    },
    {
      "number": 4,
      "arabic": "وَلَاۤ أَنَا۠ عَابِدࣱ مَّا عَبَدتُّمۡ\n",
      "translation": "Nor will I be a worshipper of what you worship.",
      "audioFile": "audio/109/109004.mp3"
    },
    {
      "number": 5,
      "arabic": "وَلَاۤ أَنتُمۡ عَـٰبِدُونَ مَاۤ أَعۡبُدُ\n",
      "translation": "Nor will you be worshippers of what I worship.",
      "audioFile": "audio/109/109005.mp3"
    },
    {
      "number": 6,
      "arabic": "لَكُمۡ دِینُكُمۡ وَلِیَ دِینِ\n",
      "translation": "For you is your religion, and for me is my religion.\"",
      "audioFile": "audio/109/109006.mp3"
    }
  ]
}