{
  "surah": 114,
  "name": "An-Naas",
  "arabicName": "سُورَةُ النَّاسِ",
  "verses": [
    {
      "number": 1,
      "arabic": "  قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ\n",
      "translation": "Say, \"I seek refuge in the Lord of mankind,",
      "transliteration": "Qul a'uzu birabbin naas"
    },
    {
      "number": 2,
      "arabic": "مَلِكِ ٱلنَّاسِ\n",
      "translation": "The Sovereign of mankind.",
      "transliteration": "Malikin naas"
    },
    {
      "number": 3,
      "arabic": "إِلَـٰهِ ٱلنَّاسِ\n",
      "translation": "The God of mankind,",
      "transliteration": "Ilaahin naas"
    },
    {
      "number": 4,
      "arabic": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ\n",
      "translation": "From the evil of the retreating whisperer -",
      "transliteration": "Min sharril was waasil khannaas"
    },
    {
      "number": 5,
      "arabic": "ٱلَّذِی یُوَسۡوِسُ فِی صُدُورِ ٱلنَّاسِ\n",
      "translation": "Who whispers [evil] into the breasts of mankind -",
      "transliteration": "Al lazee yuwas wisu fee sudoorin naas"
    },
    {
      "number": 6,
      "arabic": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "From among the jinn and mankind.\"",
      "transliteration": "Minal jinnati wan naas"
    }
  ]
}