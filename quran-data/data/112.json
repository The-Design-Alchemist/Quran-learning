{
  "surah": 112,
  "name": "Al-Ikhlaas",
  "arabicName": "سُورَةُ الإِخۡلَاصِ",
  "verses": [
    {
      "number": 1,
      "arabic": "  قُلۡ هُوَ ٱللَّهُ أَحَدٌ\n",
      "translation": "Say, \"He is Allah, [who is] One,",
      "transliteration": "Qul huwal laahu ahad"
    },
    {
      "number": 2,
      "arabic": "ٱللَّهُ ٱلصَّمَدُ\n",
      "translation": "Allah, the Eternal Refuge.",
      "transliteration": "Allah hus-samad"
    },
    {
      "number": 3,
      "arabic": "لَمۡ یَلِدۡ وَلَمۡ یُولَدۡ\n",
      "translation": "He neither begets nor is born,",
      "transliteration": "Lam yalid wa lam yoolad"
    },
    {
      "number": 4,
      "arabic": "وَلَمۡ یَكُن لَّهُۥ كُفُوًا أَحَدُۢ\n",
      "translation": "Nor is there to Him any equivalent.\"",
      "transliteration": "Wa lam yakul-lahu kufuwan ahad"
    }
  ]
}