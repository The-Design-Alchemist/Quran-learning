{
  "number": 112,
  "name": "Al-Ikhlas",
  "totalVerses": 4,
  "verses": [
    {
      "number": 1,
      "arabic": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ قُلۡ هُوَ ٱللَّهُ أَحَدٌ\n",
      "translation": "Say, \"He is Allah, [who is] One,",
      "audioFile": "audio/112/112001.mp3"
    },
    {
      "number": 2,
      "arabic": "ٱللَّهُ ٱلصَّمَدُ\n",
      "translation": "Allah, the Eternal Refuge.",
      "audioFile": "audio/112/112002.mp3"
    },
    {
      "number": 3,
      "arabic": "لَمۡ یَلِدۡ وَلَمۡ یُولَدۡ\n",
      "translation": "He neither begets nor is born,",
      "audioFile": "audio/112/112003.mp3"
    },
    {
      "number": 4,
      "arabic": "وَلَمۡ یَكُن لَّهُۥ كُفُوًا أَحَدُۢ\n",
      "translation": "Nor is there to Him any equivalent.\"",
      "audioFile": "audio/112/112004.mp3"
    }
  ]
}